{
  "version": 2,
  "builds": [
    {
      "src": "website/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "website/server.js"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png|jpg|jpeg|gif|svg))$",
      "dest": "website/$1"
    },
    {
      "src": "/(.*)",
      "dest": "website/server.js"
    }
  ]
}
